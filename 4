//4)write a c program make a pallindrome number ?


#include<stdio.h>
int main(){
    int rem,num,sum=0,orig;
    
    printf("Enter number for check pallindrome ");
    scanf("%d",&num);
    orig=num;
    
    while(num != 0){
        rem=num%10;
        sum=sum*10+rem;
        num=num/10;
    }
    
    if(sum==orig){
        printf("%d number is pallindrome",orig);
    }else{
        printf("%d number is not pallindrome",orig);
    }
    return 0;
}