//5)write a C program make a even and odd  number from starting point and ending point give user input ?



#include <stdio.h>
int even(int);
int odd(int);
int main()
{ int s,e;
    printf("enter starting and ending number : ");
  scanf("%d %d",&s,&e);
  for(int i=s;i<=e;i++){
      if(i%2==0){
          printf("%d is even \n",even(i));
      }else if(i%2 !=0){
           printf("%d is odd \n",odd(i));
      }else{
          printf("input mismatch");
      }
  }
  
    return 0;
}

int even(int a){
    return a;
}
int odd(int a){
    return a;
}